<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ksrcb.api.biz.mapper.ConfigCenterMapper">
    <resultMap id="systemList" type="map">
        <result column="system_no" property="System" jdbcType="VARCHAR" />
        <result column="env_no" property="EnvNo" jdbcType="VARCHAR" />
        <result column="env_name" property="EnvName" jdbcType="VARCHAR" />
    </resultMap>
    <select id="qryLinage" parameterType="map" resultMap="systemList">
        SELECT
               distinct system_no
        from
             environment
    </select>

    <select id="qryEnv" parameterType="map" resultMap="systemList">
        SELECT env_no,env_name
        from
             environment
        where
              system_no = #{system_no}
    </select>

</mapper>